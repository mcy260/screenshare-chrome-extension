{
    "name" : "Lernant Screenshare Extension",
    "author": "Lernant Inc",
    "version" : "0.0.6",
    "manifest_version" : 2,
    "minimum_chrome_version": "34",
    "description" : "Chrome Extension granting WebRTC screen capturing capabilities to Lernant meetings.",
    "homepage_url": "https://lernant.com/",
    "background": {
        "scripts": ["background-script.js"],
        "persistent": true
    },
    "externally_connectable": {
       "matches": [
           "*://*.lernant.com/*"
         ]
    },
    "icons" : {
        "48" : "icon.png"
    },
    "permissions": [
        "desktopCapture"
    ],
    "web_accessible_resources": [
        "icon.png"
    ]
}
